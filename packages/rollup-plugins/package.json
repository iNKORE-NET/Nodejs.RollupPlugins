{
  "name": "@inkore/rollup-plugins",
  "description": "A collection of some commonly used rollup plugins.",
  "version": "0.1.0",
  "author": "NotYoojun.!",
  "license": "LGPL-2.1-or-later",
  "keywords": [
    "rollup-plugin",
    "rollup",
    "inkore",
    "plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "A collection of some commonly used rollup plugins."
  },
  "homepage": "https://github.com/iNKORE-Public/Rollup.Plugins#readme",
  "bugs": "https://github.com/iNKORE-Public/Rollup.Plugins/issues",
  "scripts": {
    "check": "tsc",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "watch": "npm run build --watch"
  },
  "peerDependencies": {
    "rollup": "2.x || 3.x || 4.x"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "javascript-obfuscator": "^4.1.0",
    "magic-string": "^0.30.5",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "rollup": "^4.9.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ]
}
