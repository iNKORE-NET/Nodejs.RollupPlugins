{
    "name": "@inkore/rollup-plugins",
    "description": "A collection of some commonly used rollup plugins.",
    "version": "0.1.5",
    "author": "NotYoojun.!",
    "license": "LGPL-2.1-or-later",
    "keywords": [
        "rollup-plugin",
        "rollup",
        "inkore",
        "plugin"
    ],
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "A collection of some commonly used rollup plugins."
    },
    "homepage": "https://github.com/iNKORE-Public/Rollup.Plugins#readme",
    "bugs": "https://github.com/iNKORE-Public/Rollup.Plugins/issues",
    "scripts": {
        "check": "tsc",
        "build": "tsup source/index.ts --dts --format cjs,esm",
        "dev": "pnpm build --watch"
    },
    "peerDependencies": {
        "glob": "^10.3.15",
        "javascript-obfuscator": "^4.1.0",
        "postcss": "^8.4.38",
        "postcss-modules": "^6.0.0",
        "rollup": "2.x || 3.x || 4.x",
        "sass": "^1.77.1"
    },
    "peerDependenciesMeta": {
        "postcss": {
            "optional": true
        },
        "postcss-modules": {
            "optional": true
        },
        "sass": {
            "optional": true
        }
    },
    "dependencies": {
        "@niceties/logger": "^1.1.13",
        "@rollup/pluginutils": "^5.1.0",
        "acorn": "^8.11.3",
        "acorn-typescript": "^1.4.13",
        "acorn-walk": "^8.3.3",
        "colorette": "^2.0.20",
        "estree-walker": "^3.0.3",
        "fs-extra": "^11.2.0",
        "globby": "^14.0.1",
        "is-plain-object": "^5.0.0",
        "magic-string": "^0.30.5",
        "perf-regexes": "^1.0.1",
        "rimraf": "^5.0.5",
        "skip-regex": "^1.0.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.12.5",
        "eslint": "^9.7.0",
        "glob": "^10.3.15",
        "rollup": "^4.9.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "files": [
        "dist",
        "src/**/*",
        "LICENSE",
        "README.md",
        "CHANGELOG.md"
    ]
}
